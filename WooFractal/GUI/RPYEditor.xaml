<UserControl x:Class="WooFractal.RPYEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="94" d:DesignWidth="171">
    <Grid Height="94" Width="168" Background="White" AllowDrop="True" LostFocus="Grid_LostFocus">
        <Label Content="Roll" Height="25" HorizontalAlignment="Left" Margin="9,10,0,0" Name="label1" VerticalAlignment="Top" Width="54" />
        <Label Content="Pitch" Height="25" HorizontalAlignment="Left" Margin="9,34,0,0" Name="label2" VerticalAlignment="Top" Width="54" />
        <Label Content="Yaw" Height="25" HorizontalAlignment="Left" Margin="9,58,0,0" Name="label3" VerticalAlignment="Top" Width="54" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="51,12,0,0" Name="slider1" VerticalAlignment="Top" Width="100" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="51,36,0,0" Name="slider2" VerticalAlignment="Top" Width="100" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="51,60,0,0" Name="slider3" VerticalAlignment="Top" Width="100" />
    </Grid>
</UserControl>
